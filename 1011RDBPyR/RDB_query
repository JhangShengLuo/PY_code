select * from totala;


select YMD, roadname, direct, timezone, sum(carvolume), Weekday from totala where(roadname='堤頂大道' and direct='往南' and timezone='下午尖峰')group by YMD,Weekday;

select YMD, roadname, direct, timezone, sum(carvolume) volume, Weekday, rain from totala where(roadname='堤頂大道' and direct='往南' and timezone='下午尖峰')group by YMD,Weekday,rain;


select YMD, roadname, direct, timezone, sum(carvolume) volume, Weekday, rain from totala where(roadname='堤頂大道' and direct='往北' and timezone='下午尖峰')group by YMD,Weekday,rain;


select YMD, roadname, direct, timezone, sum(carvolume) volume, Weekday, rain from totala where(roadname='堤頂大道' and direct='往北' and timezone='上午尖峰')group by YMD,Weekday,rain;


